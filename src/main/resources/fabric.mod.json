	"accessWidener": "kiwi.accesswidener",
	"depends": {
		"fabric-api": "*",
		"minecraft": ">=1.20.5-",
		"java": ">=17"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/rRBvGkz"
			},
			"badges": [
				"library"
			]
